<root>


     <rule url="http://rotator.webexpert.ru/admin/start.php?m=login">
        <action isajax="false" url="http://rotator.webexpert.ru/admin/start.php" />
        <parameters_for_post>
            <field htmlname='password' htmltype='password' required='true'>
                <userdataproject name='password' />
            </field>
            <field htmlname='login' htmltype='text' required='true'>
                <userdataproject name='login' />
            </field>
        </parameters_for_post>

        <response url="http://rotator.webexpert.ru/admin/start.php" value="start.php?m=login" />
    </rule>


</root>
